	Keywords = {*Language, Adult, Aged, Analysis of Variance, Aphasia, Broca/etiology/*physiopathology/psychology, Comprehension, Corpus Striatum/*physiopathology, Female, Humans, Huntington Disease/complications/physiopathology/psychology, Male, Middle Aged, Neurodegenerative Diseases/complications/*physiopathology/psychology, Parkinson Disease/complications/physiopathology/psychology, semantics, Stroke/complications/physiopathology/psychology},
	Language = {eng},
