	Journal = {Proc Natl Acad Sci U S A},
	Keywords = {*Brain Mapping, *Language, *Motor Cortex/anatomy \& histology/physiology, Adult, Brain/anatomy \& histology/physiology, Electromyography, Female, Humans, Magnetic Resonance Imaging, Male, speech, Speech Perception/*physiology, Verbal Behavior/physiology},
