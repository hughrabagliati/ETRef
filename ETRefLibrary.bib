@article{zhou_relative_2000,
	Abstract = {The relative time course of semantic and phonological activation was investigated in the context of whether phonology mediates access to lexical representations in reading Chinese. Compound words (Experiment 1) and single-character words (Experiments 2 and 3) were preceded by semantic and phonological primes. Strong semantic priming effects were found at both short (57 ms) and long (200 ms) stimulus onset asynchrony (SOA), but phonological effects were either absent in lexical decision (Experiment 1), were present only at the longer SOA in character decision (Experiment 2) or were equally strong as semantic effects in naming (Experiment 3). Experiment 4 revealed facilitatory or inhibitory effects, depending on SOA, in phonological judgments to character pairs that were not phonologically but semantically related. It was concluded that, in reading Chinese, semantic information in the lexicon is activated at least as early and just as strongly as phonological information.},
	Annote = {Zhou, XMarslen-Wilson, WResearch Support, Non-U.S. Gov'tUnited statesJournal of experimental psychology. Learning, memory, and cognitionJ Exp Psychol Learn Mem Cogn. 2000 Sep;26(5):1245-65.},
	Author = {Zhou, X. and Marslen-Wilson, W.},
