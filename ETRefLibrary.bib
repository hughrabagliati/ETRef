	Issn = {Print 0885-2014 Elsevier Science Electronic, Print Electronic},
	Journal = {Cognitive Development},
	Keywords = {Cognitive \& Perceptual Development [2820], mother-child conversations, categorical reasoning, preschool children,, picture representations, object representations, item complexity,, talk about individuals, kinds talk, conceptual distinction},
