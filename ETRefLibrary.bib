	Journal = {Proc Natl Acad Sci U S A},
	Keywords = {*Language, Adult, Aged, Brain Mapping, Brain/pathology/*physiology/physiopathology, Cognition/physiology, Humans, Language Tests, Middle Aged},
	Language = {eng},
