	Keywords = {files, infants' representations, knowledge, perceived rigidity and cohesiveness, small groups of discrete entities, tracking},
	Language = {English},
	Month = oct,
	Pages = {203--221},
	Title = {Objects are individuals but stuff doesn't count: perceived rigidity and cohesiveness influence infants' representations of small groups of discrete entities},
