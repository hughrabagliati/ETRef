	Author = {Phillips, A. T. and Wellman, H. M.},
	Journal = {Cognition},
	Keywords = {*Child Development, *Habituation (Psychophysiology), *Intention, *Social Perception, Female, Humans, Infant, Male, Research Support, N.I.H., Extramural, Single-Blind Method, Statistics, Nonparametric},
