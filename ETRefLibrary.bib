	Issn = {1873-6246 (Electronic)},
	Journal = {Biol Psychol},
	Keywords = {*Electroencephalography, *Psycholinguistics, Algorithms, Decision Making/physiology, Evoked Potentials/*physiology, Female, Humans, Language, Linear Models, Magnetoencephalography, Male, Psychomotor Performance/physiology, Reading, Young Adult},
	Language = {eng},
	Month = jan,
