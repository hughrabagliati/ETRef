	Keywords = {*Reading, Adult, Brain/physiology, Case-Control Studies, Dyslexia/*physiopathology, Female, Humans, Magnetoencephalography, Male, Mental Processes/*physiology, Reference Values, Research Support, Non-U.S. Gov't},
	Month = aug,
