	Journal = {Cognit Psychol},
	Keywords = {*Learning, animals, cognition, Female, Language, Male, Phonetics, Psycholinguistics/*statistics \& numerical data, Psychological Theory, Saguinus, Speech perception, Vocabulary},
	Month = sep,
	Pages = {85--117},
	Title = {Learning at a distance {II}. {Statistical} learning of non-adjacent dependencies in a non-human primate},
	Volume = {49},
	Year = {2004}}

