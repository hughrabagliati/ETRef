	Journal = {Brain Res Cogn Brain Res},
	Keywords = {Acoustic Stimulation, Adult, Auditory Cortex/*physiology, Evoked Potentials, Auditory/physiology, Female, Functional Laterality/physiology, Humans, Magnetoencephalography, Male, Middle Aged, Psychomotor Performance/physiology, Sound Localization/*physiology, Space Perception/*physiology},
	Month = aug,
