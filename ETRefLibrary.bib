	Keywords = {developmental differences, eye-movements, eye movements, language development, preverbal speech segmentation, school-age-children, spoken language comprehension, syntactic ambiguity resolution, time-course, verbs, word recognition, working-memory, young infants},
	Language = {English},
	Month = nov,
