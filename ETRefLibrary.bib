	Journal = {Philos Trans R Soc Lond B Biol Sci},
	Keywords = {*Language, Brain/*physiology, Cognition/physiology, Humans, Models, Neurological, Psycholinguistics/*methods},
	Language = {eng},
	Month = may,
	Pages = {823--36},
	Title = {Morphology, language and the brain: the decompositional substrate for language comprehension},
	Volume = {362},
