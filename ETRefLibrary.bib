	Keywords = {*Semantics, Aged, Aphasia, Broca/*pathology/*physiopathology, Encephalitis, Herpes Simplex/*pathology/*physiopathology, Humans, Infarction, Middle Cerebral Artery/pathology/physiopathology, Magnetic Resonance Imaging, Middle Aged, Temporal Lobe/pathology/physiopathology, Tomography, Emission-Computed, Tomography, X-Ray Computed},
