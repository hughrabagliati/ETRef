	Issn = {0093-934X (Print)},
	Journal = {Brain Lang},
	Keywords = {*Cognition, Aphasia, Broca/*psychology, Humans, Language Tests, Neuropsychological Tests},
	Language = {eng},
	Month = apr,
	Pages = {134--47},
	Title = {The critical role of group studies in neuropsychology: comprehension regularities in {Broca}'s aphasia},
	Volume = {67},
