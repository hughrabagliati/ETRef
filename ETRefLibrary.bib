	Issn = {0749-596X},
	Journal = {Journal of Memory and Language},
	Keywords = {access, grammatical gender, lemma retrieval, models, persistence, picture-word interference, speech production, spoken sentence production, spreading-activation theory, time-course},
	Language = {English},
	Month = aug,
