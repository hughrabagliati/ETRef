	Keywords = {*Models, Neurological, *Reading, Eye Movements/*physiology, Fixation, Ocular/*physiology, Humans, Models, Psychological, Oculomotor Muscles/*physiology, Verbal Behavior/physiology, Vision, Binocular/physiology},
