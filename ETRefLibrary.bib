	Journal = {Cognition},
	Keywords = {complexity, Comprehension, constraints, discourse, eye-movements, finnish, lexical frequency, relative clauses, scrambling, semantic influences, sentence processing, syntactic ambiguity resolution, thematic role information, time-course, word order},
	Language = {English},
