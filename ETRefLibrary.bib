	Issn = {0042-6989},
	Journal = {Vision Research},
	Keywords = {aperture problem, coherent motion, connections, cortex, depth, integration, monkey, motion, occlusion, organization, surface interpretation, transparency},
	Language = {English},
	Month = feb,
	Pages = {313--328},
	Title = {Surface segmentation mechanisms and motion perception},
