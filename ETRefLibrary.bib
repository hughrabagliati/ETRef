	Keywords = {*Cognition, *Habituation (Psychophysiology), attention, Female, Humans, Infant, Infant Behavior/*psychology, Longitudinal Studies, Male, Research Support, Non-U.S. Gov't, Visual Perception},
	Month = nov,
	Pages = {1--34},
	Title = {Infants selectively encode the goal object of an actor's reach},
