	Issn = {0010-0277},
	Journal = {Cognition},
	Keywords = {acquisition, adults, certainty, evidentiality, false belief, identify, knowledge, language acquisition, language and thought, modality, representation, semantics pragmatics, theory of mind, understand, young childrens ability},
