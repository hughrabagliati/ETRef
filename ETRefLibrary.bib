	Issn = {0010-0277},
	Journal = {Cognition},
	Keywords = {adults, children, counting, discrimination, human infants, knowledge, number development, number words, numerical cognition, numerical competence, object-files, representations, systems},
	Language = {English},
	Month = nov,
	Pages = {395--438},
