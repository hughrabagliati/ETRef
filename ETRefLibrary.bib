	Author = {Branigan, H. P. and Pickering, M. J. and Tanaka, M.},
	Doi = {DOI 10.1016/j.lingua.2007.02.003},
	Issn = {0024-3841},
	Journal = {Lingua},
	Keywords = {accessibility, animacy, conceptual accessibility, constituent structure, determinants, english, formulation, grammatical functions, language production, lexical access, sentence production, spanish, syntax, word order},
	Language = {English},
