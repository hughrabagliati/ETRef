	Keywords = {*Linguistics, *Magnetic Resonance Imaging, Adult, Brain/anatomy \& histology/physiology, Evoked Potentials/*physiology, Female, Humans, Laterality/physiology, Male, Speech Perception/*physiology, Vocabulary},
	Month = dec,
