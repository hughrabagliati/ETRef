	Issn = {0305-0009},
	Journal = {Journal of Child Language},
	Keywords = {acquisition, categories, Comprehension, Language, principle},
	Language = {English},
	Month = jun,
	Pages = {267--291},
	Title = {Why is a pomegranate an apple? {The} role of shape, taxonomic relatedness, and prior lexical knowledge in children's overextensions of apple and dog},
