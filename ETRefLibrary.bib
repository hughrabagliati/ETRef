	Journal = {Hum Brain Mapp},
	Keywords = {Adolescent, Adult, Brain Mapping, Electroencephalography, Evoked Potentials, Visual/*physiology, Female, Functional Laterality/physiology, Humans, Magnetic Resonance Imaging, Male, Models, Neurological, Parietal Lobe/anatomy \& histology/*physiology, Pattern Recognition, Visual/*physiology, Photic Stimulation, Reaction Time/*physiology, Visual Cortex/anatomy \& histology/*physiology, Visual Pathways/*physiology},
