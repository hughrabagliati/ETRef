	Keywords = {Acoustic Stimulation, Adult, Algorithms, Artifacts, Auditory Cortex/anatomy \& histology/physiology, Electroencephalography/*statistics \& numerical data, Evoked Potentials, Auditory/physiology, Fractals, Humans, Magnetoencephalography/*statistics \& numerical data, Male, Neurons/*physiology},
	Month = nov,
